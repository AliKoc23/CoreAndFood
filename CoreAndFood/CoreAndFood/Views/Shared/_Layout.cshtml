<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>

    <nav class="navbar navbar-expand-sm navbar-dark bg-dark">
        <div class="container-fluid">
            <a class="navbar-brand" href="/Category/Index"> CoreAndFood</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#mynavbar">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="mynavbar">
                <ul class="navbar-nav me-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="/Category/Index">Category</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Food/Index">Food</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Chart/Index">Google Pie Chaart</a>
                    </li>
                     <li class="nav-item">
                        <a class="nav-link" href="/Chart/IndexColumn">Google Column Chaart</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Chart/Index3">Google Dynamic Chaart</a>
                    </li>
                     <li class="nav-item">
                        <a class="nav-link" href="/Chart/Statistics">Statistics</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/Icerik/Index">Tarif</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="~/Login/LogOut">Log Out</a>
                    </li>
                </ul>
                <form class="d-flex">
                    <input class="form-control me-2" type="text" placeholder="Search">
                    <button class="btn btn-primary" type="button">Search</button>
                </form>
            </div>
        </div>
    </nav>

    <div class="container-fluid mt-3">
        @RenderBody()
    </div>

</body>
</html>